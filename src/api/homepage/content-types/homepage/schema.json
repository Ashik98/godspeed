{
  "kind": "singleType",
  "collectionName": "homepages",
  "info": {
    "singularName": "homepage",
    "pluralName": "homepages",
    "displayName": "Homepage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "hero": {
      "type": "dynamiczone",
      "components": [
        "sections.hero-section"
      ]
    },
    "aboutSection": {
      "type": "dynamiczone",
      "components": [
        "sections.about-section"
      ]
    },
    "recentWorkHeading": {
      "type": "string",
      "default": "Recent Work"
    },
    "recentWorkDescription": {
      "type": "text"
    },
    "showFeaturedProjects": {
      "type": "boolean",
      "default": true
    },
    "featuredProjectCount": {
      "type": "integer"
    },
    "clientLogosHeading": {
      "type": "text"
    },
    "showClientLogos": {
      "type": "boolean",
      "default": true
    },
    "servicesHeading": {
      "type": "string",
      "default": "Services"
    },
    "servicesDescription": {
      "type": "text"
    },
    "insightsSection": {
      "type": "dynamiczone",
      "components": [
        "sections.insights-preview"
      ]
    },
    "contactSection": {
      "type": "dynamiczone",
      "components": [
        "sections.contact-section"
      ]
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    }
  }
}
